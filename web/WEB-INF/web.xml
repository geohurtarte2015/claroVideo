<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>ServletTransactionVas</servlet-name>
        <servlet-class>controlador.ServletTransactionVas</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletGraphs</servlet-name>
        <servlet-class>controlador.ServletGraphs</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Sesion</servlet-name>
        <servlet-class>controlador.Sesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EndSesion</servlet-name>
        <servlet-class>controlador.EndSesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserManagement</servlet-name>
        <servlet-class>controlador.UserManagement</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletListUsers</servlet-name>
        <servlet-class>controlador.ServletListUsers</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletGetUser</servlet-name>
        <servlet-class>controlador.ServletGetUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletUpdateUser</servlet-name>
        <servlet-class>controlador.ServletUpdateUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletDeleteUser</servlet-name>
        <servlet-class>controlador.ServletDeleteUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletTransactionVas</servlet-name>
        <url-pattern>/ServletTransactionVas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletGraphs</servlet-name>
        <url-pattern>/ServletGraphs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Sesion</servlet-name>
        <url-pattern>/Sesion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EndSesion</servlet-name>
        <url-pattern>/EndSesion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserManagement</servlet-name>
        <url-pattern>/UserManagement</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletListUsers</servlet-name>
        <url-pattern>/ServletListUsers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletGetUser</servlet-name>
        <url-pattern>/ServletGetUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletUpdateUser</servlet-name>
        <url-pattern>/ServletUpdateUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletDeleteUser</servlet-name>
        <url-pattern>/ServletDeleteUser</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            10
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
